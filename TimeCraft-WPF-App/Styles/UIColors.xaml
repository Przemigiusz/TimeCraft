<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#212529"/>
    <SolidColorBrush x:Key="SecondaryBackgroundColor" Color="#2e3338"/>

    <SolidColorBrush x:Key="PrimaryHighlightColor" Color="#495057"/>
    <SolidColorBrush x:Key="SecondaryHighlightColor" Color="#6c757d"/>

    <SolidColorBrush x:Key="PrimaryBorderHighlightColor" Color="#FFD60A"/>
    <SolidColorBrush x:Key="SecondaryBorderHighlightColor" Color="#FFC300"/>


    <SolidColorBrush x:Key="PrimaryTextColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SecondaryTextColor" Color="#adb5bd"/>
    <SolidColorBrush x:Key="TertiaryTextColor" Color="#242423"/>

</ResourceDictionary>